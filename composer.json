{
  "name": "zeus-helpdesk/zeus",
  "description": "Help Desk for contract IT departments",
  "keywords": [
    "help-desk"
  ],
  "license": "AGPL-3.0-or-later",
  "type": "project",
  "require": {
    "php": "^7.2.0",
    "ext-gd": "*",
    "ext-gmp": "*",
    "ext-json": "*",
    "barryvdh/laravel-cors": "^0.11.0",
    "beyondcode/laravel-websockets": "^1.0",
    "binarytorch/larecipe": "^1.0",
    "fideloper/proxy": "^4.0",
    "graham-campbell/guzzle-factory": "^3.0",
    "graham-campbell/markdown": "^10.0",
    "jacobbennett/laravel-http2serverpush": "^2.0",
    "laravel/framework": "5.7.*",
    "laravel/horizon": "^1.3",
    "laravel/passport": "^6.0",
    "laravel/scout": "^5.0",
    "laravel/tinker": "^1.0",
    "league/flysystem-aws-s3-v3": "~1.0",
    "league/flysystem-cached-adapter": "~1.0",
    "niiknow/bayes": "^1.0",
    "nuwave/lighthouse": "^2.6",
    "owen-it/laravel-auditing": "^8.0",
    "predis/predis": "^1.1",
    "pusher/pusher-php-server": "~3.0",
    "sentry/sentry-laravel": "1.0.0-beta2",
    "spatie/laravel-backup": "^5.10",
    "vinkla/hashids": "^5.0",
    "webuni/commonmark-table-extension": "^0.8.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.4",
    "barryvdh/laravel-translation-manager": "^0.5.2",
    "codedungeon/phpunit-result-printer": "^0.19.12",
    "doctrine/dbal": "^2.8",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "laravel/dusk": "^4.0",
    "laravel/telescope": "^0.1.3",
    "mll-lab/laravel-graphql-playground": "^0.3.3",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "nunomaduro/larastan": "^0.2.4",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
